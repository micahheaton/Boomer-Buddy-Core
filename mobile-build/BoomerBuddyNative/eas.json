{
  "cli": { 
    "version": ">= 5.0.0" 
  },
  "build": {
    "release-apk": {
      "android": {
        "workflow": "generic",
        "gradleCommand": ":app:assembleRelease",
        "artifactPath": "android/app/build/outputs/apk/release/app-release.apk"
      }
    },
    "debug-apk": {
      "android": {
        "workflow": "generic",
        "gradleCommand": ":app:assembleDebug",
        "artifactPath": "android/app/build/outputs/apk/debug/app-debug.apk"
      }
    },
    "production": {
      "android": {
        "workflow": "generic",
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}